{
  "name": "sketchprompt",
  "publisher": "PascalX",
  "displayName": "SketchPrompt",
  "description": "Visual thinking for coding with AI. Sketch ideas and use them in your prompts to improve your workflow with AI models. Works in OpenVSX-compatible editors like Cursor, Windsurf, Firebase Studio, etc. Contact: hey@sketch-prompt.com | YouTube: https://www.youtube.com/@SketchPrompt",
  "version": "0.2.6",
  "icon": "media/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/pascalx-git/SketchPrompt.git"
  },
  "license": "MIT",
  "homepage": "https://sketch-prompt.com",
  "bugs": {
    "url": "https://github.com/pascalx-git/SketchPrompt/issues"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other",
    "Visualization",
    "Productivity"
  ],
  "keywords": [
    "sketching",
    "drawing",
    "visual",
    "design",
    "diagrams",
    "prompts",
    "ai",
    "collaboration",
    "cursor",
    "ux",
    "visual-thinking",
    "sketch",
    "tldraw",
    "brainstorming",
    "wireframes",
    "prototyping"
  ],
  "markdown": "github",
  "galleryBanner": {
    "color": "#3b82f6",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/badge/version-0.2.5-blue.svg?labelColor=%23cccccc&color=%233b82f6",
      "href": "https://github.com/pascalx-git/SketchPrompt",
      "description": "Version"
    },
    {
      "url": "https://img.shields.io/badge/license-MIT-green.svg?labelColor=%23cccccc&color=%233b82f6",
      "href": "https://github.com/pascalx-git/SketchPrompt/blob/main/LICENSE",
      "description": "License"
    },
    {
      "url": "https://img.shields.io/open-vsx/dt/PascalX/SketchPrompt?labelColor=%23cccccc&color=%233b82f6",
      "href": "https://open-vsx.org/extension/PascalX/SketchPrompt",
      "description": "Open VSX Downloads"
    },
    {
      "url": "https://img.shields.io/open-vsx/stars/PascalX/SketchPrompt?labelColor=%23cccccc&color=%233b82f6",
      "href": "https://open-vsx.org/extension/PascalX/SketchPrompt",
      "description": "Open VSX Rating"
    },
    {
      "url": "https://img.shields.io/open-vsx/release-date/PascalX/SketchPrompt?labelColor=%23cccccc&color=%233b82f6",
      "href": "https://open-vsx.org/extension/PascalX/SketchPrompt",
      "description": "Open VSX Release Date"
    },
    {
      "url": "https://img.shields.io/github/commit-activity/m/PascalX-Git/SketchPrompt?labelColor=%23cccccc&color=%233b82f6",
      "href": "https://github.com/pascalx-git/SketchPrompt",
      "description": "GitHub commit activity"
    }
  ],
  "publisherDisplayName": "PascalX",
  "activationEvents": [
    "onCommand:sketchprompt.newSketch",
    "onCommand:sketchprompt.help",
    "onCommand:sketchprompt.feedback",
    "onCommand:sketchprompt.generatePreview",
    "onCommand:sketchprompt.openCanvas",
    "onCommand:workbench.action.openWith",
    "onLanguage:sketchprompt"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sketchprompt.newSketch",
        "title": "SketchPrompt: New Sketch"
      },
      {
        "command": "sketchprompt.help",
        "title": "SketchPrompt: Help"
      },
      {
        "command": "sketchprompt.feedback",
        "title": "SketchPrompt: Share Feedback"
      },
      {
        "command": "sketchprompt.generatePreview",
        "title": "SketchPrompt: Generate AI Preview"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "sketchprompt.newSketch",
          "group": "navigation",
          "when": "workspaceFolderCount > 0"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "sketchprompt.editor",
        "displayName": "SketchPrompt Editor",
        "selector": [
          {
            "filenamePattern": "**/*.sketchprompt"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "sketchprompt",
        "aliases": [
          "SketchPrompt"
        ],
        "extensions": [
          ".sketchprompt"
        ],
        "configuration": "./sketchprompt-language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run build-webview && npm run bundle && npm run copy-resources",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build-webview": "vite build",
    "watch-webview": "vite build --watch",
    "bundle": "webpack --mode production",
    "copy-resources": "mkdir -p resources && (cp SketchPrompt/demo.sketchprompt resources/demo.sketchprompt 2>/dev/null || true)",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.19.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^8.0.0",
    "glob": "^8.0.0",
    "mocha": "^10.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.3.5",
    "vsce": "^2.15.0",
    "webpack": "^5.100.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tldraw": "^3.13.4"
  }
}
